set encoding=utf-8
set fileencoding=utf-8
set fileformat=unix
set backspace=2
set tabstop=2
set softtabstop=2
set shiftwidth=2
set noexpandtab
set autoindent
set copyindent
set preserveindent
set number
set statusline=%y\ %t\ %l:%v
set laststatus=2
set colorcolumn=80
set background=dark
let c_syntax_for_h=1
let c_no_trail_space_error=1
let c_no_tab_space_error=1
let c_no_curly_error=1
let c_no_bracket_error=1
syntax on
colorscheme protovision 

