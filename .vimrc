set encoding=utf-8
set fileencoding=utf-8
set fileformat=unix
set backspace=2
set tabstop=2
set softtabstop=2
set shiftwidth=2
set noexpandtab
set autoindent
set copyindent
set preserveindent
set number
set statusline=%y\ %t\ %l:%v
set laststatus=2
set colorcolumn=80
set background=dark
let g:c_syntax_for_h=1
syntax on
colorscheme protovision 

